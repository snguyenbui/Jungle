<h1>Admin Dashboard</h1>
<h2>
  Number of categories in the database
  <%= @category.count %>
</h2>
  <% @category.each do |cat| %>
    <%= cat.name %><br>
  <% end %>
<h2>
  Number of products in the database
  <%= @products.size %>
</h2>
  <% @products.each do |product| %>
    <%= product.name %><br>
  <% end %>